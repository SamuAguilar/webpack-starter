    "css-minimizer-webpack-plugin": "^7.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.2",
    "image-minimizer-webpack-plugin": "^4.1.0",
    "image-webpack-loader": "^8.1.0",
    "imagemin": "^9.0.0",
    "mini-css-extract-plugin": "^2.9.1",
    "sharp": "^0.33.5",
    "style-loader": "^4.0.0",
    "svgo": "^3.3.2",
